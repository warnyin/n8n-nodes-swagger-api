{
  "name": "n8n-nodes-swagger-api",
  "version": "1.0.0",
  "description": "n8n node for integrating with REST APIs using Swagger/OpenAPI specifications",
  "license": "MIT",
  "author": {
    "name": "warnyin"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/warnyin/n8n-nodes-swagger-api.git"
  },
  "bugs": {
    "url": "https://github.com/warnyin/n8n-nodes-swagger-api/issues"
  },
  "homepage": "https://github.com/warnyin/n8n-nodes-swagger-api#readme",
  "keywords": [
    "n8n-community-node-package",
    "n8n",
    "swagger",
    "openapi",
    "api",
    "rest",
    "http"
  ],
  "main": "index.js",
  "scripts": {
    "build": "tsc && npm run copy",
    "copy": "copyfiles -u 1 \"nodes/**/*.{png,svg,json}\" dist/",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials --ext .ts",
    "lintfix": "eslint nodes credentials --ext .ts --fix",
    "prepublishOnly": "npm run build && npm run lint"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/SwaggerApiCredentials.credentials.js"
    ],
    "nodes": [
      "dist/nodes/SwaggerApi/SwaggerApi.node.js"
    ]
  },
  "devDependencies": {
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.42.0",
    "eslint-plugin-n8n-nodes-base": "^1.11.0",
    "n8n-workflow": "^1.0.0",
    "typescript": "^5.1.3",
    "copyfiles": "^2.4.1",
    "prettier": "^3.0.0"
  }
}
